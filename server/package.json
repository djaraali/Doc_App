{
  "name": "just-auth-server",
  "version": "1.0.0",
  "description": "JUST Authentication Server",
  "main": "server.js",
  "scripts": {
    "start": "yarn run prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "SET NODE_ENV=development & npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "cross-env NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "author": "Adrian",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "generate-password": "^1.4.2",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.27",
    "moment": "^2.25.3",
    "mongoose": "^5.8.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "rimraf": "^3.0.0",
    "slug": "^2.0.0",
    "useragent": "^2.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
